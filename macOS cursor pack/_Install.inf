signature="$CHICAGO$"

CopyFiles = Scheme.Cur
AddReg    = Scheme.Reg,Wreg

[DestinationDirs]
Scheme.Cur = 10,"%CUR_DIR%"

[Scheme.Reg]
HKCU,"Control Panel\Cursors\Schemes","%SCHEME_NAME%",,"%10%\%CUR_DIR%\%pointer%,%10%\%CUR_DIR%\%help%,%10%\%CUR_DIR%\%work%,%10%\%CUR_DIR%\%busy%,%10%\%CUR_DIR%\%Cross%,%10%\%CUR_DIR%\%Text%,%10%\%CUR_DIR%\%Hand%,%10%\%CUR_DIR%\%Unavailiable%,%10%\%CUR_DIR%\%Vert%,%10%\%CUR_DIR%\%Horz%,%10%\%CUR_DIR%\%Dgn1%,%10%\%CUR_DIR%\%Dgn2%,%10%\%CUR_DIR%\%move%,%10%\%CUR_DIR%\%alternate%,%10%\%CUR_DIR%\%link%,%10%\%CUR_DIR%\%Location%,%10%\%CUR_DIR%\%Person%"

[Wreg]
HKCU,"Control Panel\Cursors",,0x00020000,"%SCHEME_NAME%"
HKCU,"Control Panel\Cursors",Working In Background,0x00020000,"%10%\%CUR_DIR%\%work%"
HKCU,"Control Panel\Cursors",Normal Select,0x00020000,"%10%\%CUR_DIR%\%pointer%"
HKCU,"Control Panel\Cursors",Precision Select,0x00020000,"%10%\%CUR_DIR%\%Cross%"
HKCU,"Control Panel\Cursors",Link Select,0x00020000,"%10%\%CUR_DIR%\%link%"
HKCU,"Control Panel\Cursors",Help Select,0x00020000,"%10%\%CUR_DIR%\%Help%"
HKCU,"Control Panel\Cursors",Text Select,0x00020000,"%10%\%CUR_DIR%\%Text%"
HKCU,"Control Panel\Cursors",Unavailable,0x00020000,"%10%\%CUR_DIR%\%Unavailiable%"
HKCU,"Control Panel\Cursors",Handwriting,0x00020000,"%10%\%CUR_DIR%\%Hand%"
HKCU,"Control Panel\Cursors",Move,0x00020000,"%10%\%CUR_DIR%\%move%"
HKCU,"Control Panel\Cursors",Diagonal Resize 2,0x00020000,"%10%\%CUR_DIR%\%Dgn2%"
HKCU,"Control Panel\Cursors",Vertical Resize,0x00020000,"%10%\%CUR_DIR%\%Vert%"
HKCU,"Control Panel\Cursors",Diagonal Resize 1,0x00020000,"%10%\%CUR_DIR%\%Dgn1%"
HKCU,"Control Panel\Cursors",Horizontal Resize,0x00020000,"%10%\%CUR_DIR%\%Horz%"
HKCU,"Control Panel\Cursors",Alternate Select,0x00020000,"%10%\%CUR_DIR%\%alternate%"
HKCU,"Control Panel\Cursors",Busy,0x00020000,"%10%\%CUR_DIR%\%busy%"
HKCU,"Control Panel\Cursors",Location Select,0x00020000,"%10%\%CUR_DIR%\%Location%"
HKCU,"Control Panel\Cursors",Person Select,0x00020000,"%10%\%CUR_DIR%\%Person%"
HKLM,"SOFTWARE\Microsoft\Windows\CurrentVersion\Runonce\Setup\","",,"rundll32.exe shell32.dll,Control_RunDLL main.cpl @0,1"

[Scheme.Cur]
"Normal Select.cur"
"Help Select.cur"
"Working In Background.ani"
"Busy.ani"
"Precision Select.cur"       
"Text Select.cur"
"Handwriting.cur"
"Unavailable.cur"
"Vertical Resize.cur"
"Horizontal Resize.cur"
"Diagonal Resize 1.cur"
"Diagonal Resize 2.cur"
"Move.cur"
"Alternate Select.cur"
"Link Select.cur"
"Location Select.cur"
"Person Select.cur"

[Strings]
CUR_DIR         = "Cursors\MacOS"
SCHEME_NAME     = "MacOS"
pointer			= "Normal Select.cur"
help			= "Help Select.cur"
work			= "Working In Background.ani"
busy			= "Busy.ani"
cross			= "Precision Select.cur"
text			= "Text Select.cur"
hand			= "Handwriting.cur"
unavailiable	= "Unavailable.cur"
vert			= "Vertical Resize.cur"   
horz			= "Horizontal Resize.cur"
dgn1			= "Diagonal Resize 1.cur"
dgn2			= "Diagonal Resize 2.cur"
move			= "Move.cur"
alternate		= "Alternate Select.cur"
link			= "Link Select.cur"
location		= "Location Select.cur"
person			= "Person Select.cur"

